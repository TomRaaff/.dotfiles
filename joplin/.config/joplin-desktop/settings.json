{
	"$schema": "https://joplinapp.org/schema/settings.json",
	"editor.codeView": true,
	"sync.target": 7,
	"locale": "en_GB",
	"theme": 4,
	"themeAutoDetect": true,
	"preferredLightTheme": 1,
	"preferredDarkTheme": 4,
	"notes.sortOrder.field": "order",
	"notes.sortOrder.reverse": false,
	"notes.perFieldReverse": {
		"user_updated_time": true,
		"user_created_time": true,
		"title": false,
		"order": false,
		"todo_due": true,
		"todo_completed": true
	},
	"newNoteFocus": "title",
	"markdown.plugin.softbreaks": false,
	"markdown.plugin.typographer": false,
	"style.editor.fontSize": 16,
	"style.editor.fontFamily": "Andale mono",
	"style.editor.monospaceFontFamily": "courier",
	"ui.layout": {
		"key": "root",
		"children": [
			{
				"key": "sideBar",
				"width": 269,
				"visible": true
			},
			{
				"key": "noteList",
				"width": 331,
				"visible": true
			},
			{
				"key": "editor",
				"visible": true
			}
		],
		"visible": true
	},
	"noteVisiblePanes": [
		"viewer"
	],
	"editor.keyboardMode": "vim",
	"editor.spellcheckBeta": true,
	"api.token": "dee6f7bedb7396959baef4abff70a8d92ec209ef7ea6e493c25ca8db26b43a81c795d7d3f026cc1c076c0dae7abba943b4cbbf0a265b37007a00e7b4ae8627aa",
	"spellChecker.languages": [
		"en-GB"
	]
}
